{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-use-before-define": "off",
        "consistent-return": "off",
        "no-underscore-dangle": "off",
        "max-len": "off",
        "new-cap": "off"
    }
}